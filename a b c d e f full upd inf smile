OwO:Label("Teleport Player Get Key (Random)", Color3.fromRGB(127, 143, 166))
OwO:Button("Tp PLayer Get Key", function()
    for _, player in pairs(game:GetService("Players"):GetPlayers()) do
		if player.Backpack:FindFirstChild("Key") ~= nil or game.Workspace[player.Name]:FindFirstChild("Knife") ~= nil then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame
			break
		end
	end
end)
